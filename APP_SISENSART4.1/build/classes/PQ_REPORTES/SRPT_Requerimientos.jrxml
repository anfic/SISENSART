	<parameter name="P_rq" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
     detalle_requerimiento.`id_requerimiento` AS detalle_requerimiento_id_requerimiento,
     tipo_material.`descripcion` AS tipo_material_descripcion
FROM
     `material` material INNER JOIN `detalle_requerimiento` detalle_requerimiento ON material.`id_material` = detalle_requerimiento.`id_material`
     INNER JOIN `tipo_material` tipo_material ON material.`id_tipo` = tipo_material.`id_tipo`
WHERE detalle_requerimiento.`id_requerimiento`=$P{P_rq} GROUP BY tipo_material.`descripcion`]]>
	</queryString>
