	<queryString>
		<![CDATA[SELECT
     proveedor.`id_proveedor` AS proveedor_id_proveedor,
     proveedor.`razon` AS proveedor_razon,
     proveedor.`representante` AS proveedor_representante,
     proveedor.`ruc` AS proveedor_ruc,
     proveedor.`direccion` AS proveedor_direccion,
     proveedor.`ciudad` AS proveedor_ciudad,
     proveedor.`telefono` AS proveedor_telefono,
     proveedor.`correo` AS proveedor_correo,
     cuentas_proveedor.`cta_soles` AS cuentas_proveedor_cta_soles,
     cuentas_proveedor.`cta_dolares` AS cuentas_proveedor_cta_dolares,
     cuentas_proveedor.`cci_soles` AS cuentas_proveedor_cci_soles,
     cuentas_proveedor.`cci_dolares` AS cuentas_proveedor_cci_dolares,
     banco.`idbanco` AS banco_idbanco,
     banco.`descripcion` AS banco_descripcion,
     banco.`abeviatura` AS banco_abeviatura,
     contacto.`idcontacto` AS contacto_idcontacto,
     contacto.`nombres` AS contacto_nombres,
     contacto.`nro_cel` AS contacto_nro_cel,
     contacto.`correo` AS contacto_correo,
     configuracion_empresa.`razon_social` AS empresa_razon_social,
     configuracion_empresa.`ruc` AS empresa_ruc,
     configuracion_empresa.`direccion` AS empresa_direccion,
     configuracion_empresa.`telefono` AS empresa_telefono,
     configuracion_empresa.`representante_legal` AS empresa_representante_legal
FROM
     `proveedor` proveedor INNER JOIN `cuentas_proveedor` cuentas_proveedor ON proveedor.`id_proveedor` = cuentas_proveedor.`proveedor_id_proveedor`
     INNER JOIN `banco` banco ON cuentas_proveedor.`banco_idbanco` = banco.`idbanco`
     INNER JOIN `contacto` contacto ON proveedor.`id_proveedor` = contacto.`id_proveedor`,
     `configuracion_empresa` configuracion_empresa
WHERE
     proveedor.`id_proveedor` = $P{p_proveedor}
GROUP BY contacto.`nro_cel` and contacto.`nombres` and cuentas_proveedor.`cta_soles`]]>
	</queryString>
